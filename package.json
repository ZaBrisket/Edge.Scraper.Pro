{
  "name": "edge.scraper.pro",
  "version": "3.0.0",
  "description": "Professional web scraper with sports data extraction",
  "main": "public/index.html",
  "scripts": {
    "test": "jest",
    "build": "npm run build:functions",
    "build:functions": "cd netlify/functions && npm run build || echo 'No function build needed'",
    "serve": "npx http-server public -p 8080",
    "dev": "netlify dev",
    "clean": "rm -rf node_modules build dist",
    "lint": "eslint src --ext .js",
    "validate": "node src/lib/validators/pfr-validator.js",
    "test:nda": "vitest run tests/nda --reporter=basic",
    "test:nda:watch": "vitest tests/nda"
  },
  "dependencies": {
    "@mozilla/readability": "^0.5.0",
    "@prisma/client": "^5.20.0",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "bottleneck": "^2.19.5",
    "busboy": "^1.6.0",
    "cheerio": "^1.1.2",
    "compromise": "^14.14.4",
    "csv-parser": "^3.2.0",
    "csv-stringify": "^6.6.0",
    "date-fns": "^4.1.0",
    "diff-match-patch-es": "^1.0.1",
    "docx": "^9.5.1",
    "exceljs": "^4.4.0",
    "fast-xml-parser": "^4.4.0",
    "jsdom": "^26.1.0",
    "jszip": "^3.10.1",
    "jsonwebtoken": "^9.0.2",
    "mammoth": "^1.6.0",
