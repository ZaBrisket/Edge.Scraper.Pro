    "@types/node": "^20.12.12",
    "eslint": "^9.35.0",
    "http-server": "^14.1.1",
    "jest": "^30.1.3",
    "netlify-cli": "^17.0.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "vitest": "^1.6.0"
