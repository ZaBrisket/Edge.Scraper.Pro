{
  "name": "edge.scraper.pro",
  "version": "3.0.0",
  "description": "Professional web scraper with sports data extraction",
  "main": "public/index.html",
  "scripts": {
    "test": "npm run test:unit && npm run test:integration && npm run test:e2e && npm run test:nda && npm run test:contamination",
    "test:nda": "jest --config jest.nda.config.js --runInBand",
    "test:unit": "jest --config jest.config.js --runInBand",
    "test:integration": "playwright test tests/integration/",
    "test:e2e": "playwright test tests/e2e/",
    "test:contamination": "node tools/contamination-scan.js",
    "build": "npm run build:nda-docx && npm run build:nda && npm run build:functions",
    "build:nda-docx": "node tools/build-nda-docx.js",
    "build:nda": "node tools/build-nda-policy.js",
    "build:nonces": "node tools/build-nonces.js",
    "build:functions": "cd netlify/functions && npm run build || echo 'No function build needed'",
    "serve": "npx http-server public -p 8080",
    "dev": "netlify dev",
    "clean": "rm -rf node_modules build dist",
    "lint": "eslint src --ext .js",
    "validate": "node src/lib/validators/pfr-validator.js",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "postinstall": "npm run build:nda-docx",
    "prepare": "husky install"
  },
  "dependencies": {
    "@mozilla/readability": "^0.5.0",
    "@prisma/client": "^5.20.0",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "bottleneck": "^2.19.5",
    "cheerio": "^1.1.2",
    "compromise": "^14.14.4",
    "csv-parser": "^3.2.0",
    "csv-stringify": "^6.6.0",
    "date-fns": "^4.1.0",
    "exceljs": "^4.4.0",
    "fast-xml-parser": "^4.3.5",
    "jsdom": "^26.1.0",
    "jsonwebtoken": "^9.0.2",
    "jszip": "^3.10.1",
    "natural": "^8.1.0",
    "p-queue": "^8.1.1",
    "pino": "^8.15.0",
    "playwright": "^1.48.0",
    "set-cookie-parser": "^2.6.0",
    "undici": "^6.19.8",
    "xml2js": "^0.6.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.1",
    "@types/jest": "^30.0.0",
    "esbuild": "^0.24.0",
    "eslint": "^9.35.0",
    "http-server": "^14.1.1",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "netlify-cli": "^17.0.0",
    "ts-jest": "^29.4.4",
    "typescript": "^5.9.2"
  }
}
