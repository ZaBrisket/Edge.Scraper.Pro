{
  "$id": "trivia_v1.wr_season.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["player_id", "season", "age", "team_full", "team_abbrs", "pos", "G", "GS", "Awards", "multi_team"],
  "properties": {
    "player_id": {
      "type": "string",
      "pattern": "^[a-z0-9_]+$"
    },
    "season": {
      "type": "integer",
      "minimum": 1997,
      "maximum": 2024
    },
    "age": {
      "type": "integer",
      "minimum": 18,
      "maximum": 50
    },
    "team_full": {
      "type": "string",
      "minLength": 1
    },
    "team_abbrs": {
      "type": "string",
      "minLength": 1
    },
    "pos": {
      "type": "string",
      "const": "WR"
    },
    "G": {
      "type": "integer",
      "minimum": 0
    },
    "GS": {
      "type": "integer",
      "minimum": 0
    },
    "Awards": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "multi_team": {
      "type": "boolean"
    },
    "Tgt": {
      "type": "number",
      "minimum": 0
    },
    "Rec": {
      "type": "number",
      "minimum": 0
    },
    "RecYds": {
      "type": "number"
    },
    "Y/R": {
      "type": "number"
    },
    "RecTD": {
      "type": "number",
      "minimum": 0
    },
    "R/G": {
      "type": "number"
    },
    "RecY/G": {
      "type": "number"
    },
    "Fmb": {
      "type": "number",
      "minimum": 0
    },
    "Att": {
      "type": "number",
      "minimum": 0
    },
    "Yds": {
      "type": "number"
    },
    "TD": {
      "type": "number",
      "minimum": 0
    }
  },
  "additionalProperties": false
}